# Stripe Configuration (Test Mode for Local Development)
# Get these from: https://dashboard.stripe.com/test/apikeys
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_publishable_key_here

# Application Base URL
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Google Maps API (Optional - for contact page map)
# Get from: https://console.cloud.google.com/
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# AWS S3 Configuration
# Get these from: https://console.aws.amazon.com/iam/
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=eu-west-2
AWS_S3_BUCKET_NAME=your_bucket_name_here
NEXT_PUBLIC_AWS_CLOUDFRONT_DOMAIN=your_cloudfront_domain_here

